build_python_jmespath() {
    download_source https://files.pythonhosted.org/packages/00/2a/e867e8531cf3e36b41201936b7fa7ba7b5702dbef42922193f05c8976cd6/jmespath-1.0.1.tar.gz

    pushd build

    rm -rf jmespath-1.0.1
    tar xvf ../sources/jmespath-1.0.1.tar.gz

    setup_environment

    pushd jmespath-1.0.1
    python setup.py build
    python setup.py install --prefix=$LFS_STAGING
    popd # jmespath

    post_build

    popd # build
}
