build_python_urllib3() {
    download_source https://files.pythonhosted.org/packages/15/22/9ee70a2574a4f4599c47dd506532914ce044817c7752a79b6a51286319bc/urllib3-2.5.0.tar.gz

    pushd build

    rm -rf urllib3-2.5.0
    tar xvf ../sources/urllib3-2.5.0.tar.gz

    setup_environment

    pushd urllib3-2.5.0
    cp -Rv src/urllib3 $LFS_STAGING/usr/lib/python3.13/site-packages/
    popd # urllib3

    post_build

    popd # build
}
